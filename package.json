{
  "name": "concious",
  "version": "0.0.0",
  "description": "ReactJS based Concise UI Framework.",
  "main": "concious.js",
  "support": "concious.support.js",
  "deploy": "concious.deploy.js",
  "scripts": {
    "preinstall": "npm install babel-cli bower browser-sync gulp-cli webpack webpack-dev-server watch veon --global && bower install",
    "postinstall": "npm rebuild node-sass",
    "deploy": "gulp clean && veon set production \"gulp && babel concious.js -o concious.support.js && webpack\"",
    "build": "gulp clean && gulp && babel concious.js -o concious.support.js && webpack",
    "test": "veon set test \"gulp && webpack --test\"",
    "watch": "gulp clean && watch \"npm run test\" ./ ./*/ --filter=./.filter.js --wait=5 --interval=5 --ignoreDotFiles",
    "live": "gulp clean && gulp watch",
    "server": "veon set test \"webpack-dev-server -d --test\"",
    "browser": "browser-sync start --port 3000 --proxy \"localhost:4000\" --index=\"test-index.html\" --files \"test-index.html\" \"test.deploy.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystems/concious.git"
  },
  "keywords": [
    "concise",
    "ui",
    "framework",
    "reactjs",
    "concious"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/concious/issues"
  },
  "homepage": "https://github.com/volkovasystems/concious#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.2",
    "gulp-debug": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.3.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.11",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "vinyl-paths": "^2.1.0",
    "watch": "^1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "ceries": "^0.2.0",
    "clazof": "^0.3.0",
    "depher": "^0.5.0",
    "doubt": "^0.6.0",
    "een": "^0.1.0",
    "falze": "^0.3.0",
    "falzy": "^0.7.0",
    "harden": "^0.13.0",
    "kein": "^0.5.0",
    "kley": "^0.1.0",
    "plough": "^0.5.0",
    "protype": "^0.8.0",
    "pyck": "^0.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "shardize": "^0.5.0",
    "snapd": "^0.9.0",
    "transpher": "^0.2.0",
    "truly": "^0.5.0",
    "truu": "^0.4.0"
  }
}
